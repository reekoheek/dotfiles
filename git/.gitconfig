[push]
	default = simple
[alias]
	a = add .
	s = status
	c = commit -a
	co = checkout
	t = tag
	amend = commit --amend
	co = checkout
	ps = push
	pt = push --tags
	pl = pull
	rv = remote -v
	lg = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
[pull]
	rebase = false
[init]
	defaultBranch = main
[user]
	email = reekoheek@gmail.com
	name = Ganesha
[github]
	user = reekoheek
[core]
	editor = nvim
